{
  "name": "VK messages stats",
  "description": "VK messages stats",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' https://vk.com; object-src 'self' https://vk.com",
  "manifest_version": 2,
  "browser_action": {},
  "permissions": [
    "tabs", "*://api.vk.com/*", "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
