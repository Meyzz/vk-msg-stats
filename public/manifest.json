{
  "name": "VK messages stats",
  "description": "VK messages stats",
  "version": "0.0.2",
  "content_security_policy": "script-src 'self' https://vk.com; object-src 'self' https://vk.com",
  "manifest_version": 2,
  "browser_action": {},
  "icons": { "16": "16.png",
    "48": "48.png",
    "128": "128.png" },
  "permissions": [
    "tabs", "*://api.vk.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
